[flake8]
max-line-length = 88
exclude = 
    .git,
    __pycache__,
    venv,
    env,
    migrations,
    .venv,
    node_modules,
    .tox,
    .coverage,
    .coverage.*,
    .cache,
    nosetests.xml,
    coverage.xml,
    *.cover,
    *.log,
    .DS_Store
ignore = 
    E203,
    E501,
    W503,
    F401
per-file-ignores =
    __init__.py:F401
    settings.py:E501
    */migrations/*:E501,F401
max-complexity = 10